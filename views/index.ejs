<% extend('template/layout') %>
<div class="row">
    <div class="col">
        <h1>Detail of API</h1>
    </div>
</div>
<div class="row">
    <div class="col"></div>
</div>
<div class="row">
    <div class="col">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">URL</th>
                    <th scope="col">Action</th>
                    <th scope="col">Path Parameters</th>
                    <th scope="col">Status</th>
                    <th scope="col">Respone</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i<data.length; i++) {%>
                    <tr>
                        <td><%= data[i].URL%></td>
                        <td>
                            <button type="button" class="btn btn-success"><%= data[i].Action%>
                            </button>
                        </td>
                        <td>
                            <button type="button" class="btn btn-info">
                                <%= data[i].PathParameters%></button>
                        </td>
                        <td>
                            <button type="button" class="btn btn-warning"><%= data[i].Respone%></button>
                        </td>
                        <td>
                            <div class="form-group">
                                <textarea class="form-control"  rows="23" cols="40" readonly>
 <%= data[i].data%>
                                </textarea>
                            </div>
                        </td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</div>