            </thead>
            <tbody>
                <tr>
                    <td></td>
                    <td>
                        <button type="button" class="btn btn-success">GET</button>
                    </td>
                    <td>
                        <button type="button" class="btn btn-info">
                            Non-Required</button>
                    </td>
                    <td>
                        <button type="button" class="btn btn-warning">200 Success</button>
                    </td>
                    <td>
                        <div class="form-group">
                            <textarea class="form-control" id="exampleTextarea" rows="23" cols="40"></textarea>
